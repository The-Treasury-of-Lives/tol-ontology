PREFIX bdo: <http://purl.bdrc.io/ontology/core/>
PREFIX bdr: <http://purl.bdrc.io/resource/> 
PREFIX bf: <http://id.loc.gov/ontologies/bibframe/>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX gist: <https://w3id.org/semanticarts/ns/ontology/gist/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX told: <https://w3id.org/treasuryoflives/ns/data/tol/>
PREFIX tolo: <https://w3id.org/treasuryoflives/ns/ontology/tol/>
PREFIX tolx: <https://w3id.org/treasuryoflives/ns/taxonomy/tol/>
PREFIX xml: <http://www.w3.org/XML/1998/namespace>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX wiki: <https://www.wikidata.org/wiki/>
PREFIX media-txt: <https://www.iana.org/assignments/media-types/text/> 

## COLUMNS 

# person_id
# tbrc_id
# default_name
# tradition_id
# tradition_name (ignore)
# parent_tradition
# parent_tradition_name (ignore)

# Run: 
# tarql -e utf-8 --dedup 10000 person_traditions.tq person-tradition-4-4-24.csv > tolPersonTraditions.ttl

CONSTRUCT {

    ?person_iri 
        gist:isConnectedTo 
            ?tradition_iri ,
            ?parent_tradition_iri ;
    .
}

WHERE {
    
    # Testing
    # FILTER(?ROWNUM IN (1, 864))

    BIND(tarql:expandPrefixedName(CONCAT("told:_Person_", ?person_id))AS ?person_iri)
    BIND(tarql:expandPrefixedName(CONCAT("told:_Tradition_", ?tradition_id))AS ?tradition_iri)
    BIND(tarql:expandPrefixedName(CONCAT("told:_Tradition_", ?parent_tradition_id))AS ?parent_tradition_iri)
}
