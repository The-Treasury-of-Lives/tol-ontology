tol:primaryTitle
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf tol:title ;
	rdfs:domain tol:Person ;
	skos:definition "The primary form of a person's title."^^xsd:string ;
	skos:prefLabel "primary title"^^xsd:string ;
	.

tol:start
	a owl:DatatypeProperty ;
	skos:definition "The date, time, or datetime some event, relationship, or other thing started."^^xsd:string ;
	skos:editorialNote "TODO Re-evaluate with gist 11. EDTF is not recognized by OWL processors. How much uncertainty to we really need to accommodate?"^^xsd:string ;
	skos:prefLabel "start"^^xsd:string ;
	skos:scopeNote "May be used with edtf:EDTF datatyped strings to allow for partial, uncertain, and approximate dates."^^xsd:string ;
	.

tol:title
	a owl:DatatypeProperty ;
	rdfs:domain tol:Person ;
	rdfs:range xsd:string ;
	skos:definition "An appellation of dignity, honor, distinction, or preeminence attached to a person or family by virtue of rank, office, precedent, privilege, attainment, or lands."^^xsd:string ;
	skos:prefLabel "title"^^xsd:string ;
	.

tol:titleVariant
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf tol:title ;
	rdfs:domain tol:Person ;
	skos:definition "A variant form of a person's title."^^xsd:string ;
	skos:editoralNote "Explain more about how these differ from the primary name."^^xsd:string ;
	skos:prefLabel "title variant"^^xsd:string ;
	.

