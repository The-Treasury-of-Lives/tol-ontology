tolo:hasAunt
	a owl:ObjectProperty ;
	rdfs:subPropertyOf tolo:hasFamilialRelationshipTo ;
	owl:propertyChainAxiom (
		tolo:hasParent
		tolo:hasSister
	) ;
	skos:definition "Relates a person to his/her parent's sister."@en ;
	skos:prefLabel "has aunt"@en ;
	skos:scopeNote "In 1.0.0 we are not distinguishing biological, adoptive, half, and step relationships."@en ;
	.

tolo:hasBrother
	a owl:ObjectProperty ;
	rdfs:subPropertyOf tolo:hasSibling ;
	owl:propertyChainAxiom (
		tolo:hasParent
		tolo:hasSon
	) ;
	skos:definition "Relates a person to a son of his/her parent(s)."@en ;
	skos:prefLabel "has brother"@en ;
	skos:scopeNote "In 1.0.0 we are not distinguishing biological, adoptive, half, and step relationships."@en ;
	.

tolo:hasChild
	a owl:ObjectProperty ;
	owl:inverseOf tolo:hasParent ;
	skos:definition "Relates a person to his/her son or daughter."@en ;
	skos:prefLabel "has child"@en ;
	skos:scopeNote "In 1.0.0 we are not distinguishing biological, adoptive, half, and step relationships."@en ;
	.

