# imports: https://w3id.org/semanticarts/ontology/gistCore12.0.1

@prefix bdo: <http://purl.bdrc.io/ontology/core/> .
@prefix gist: <https://w3id.org/semanticarts/ns/ontology/gist/> .
@prefix media-txt: <https://www.iana.org/assignments/media-types/text/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix told: <https://w3id.org/treasuryoflives/ns/data/tol/> .
@prefix tolo: <https://w3id.org/treasuryoflives/ns/ontology/tol/> .
@prefix tolx: <https://w3id.org/treasuryoflives/ns/taxonomy/tol/> .
@prefix wiki: <https://www.wikidata.org/wiki/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://w3id.org/treasuryoflives/ontology/tolCoreX.x.x>
	a owl:Ontology ;
	owl:imports <https://w3id.org/semanticarts/ontology/gistCore12.0.1> ;
	owl:versionIRI <https://w3id.org/treasuryoflives/ontology/tolCoreX.x.x> ;
	skos:definition "The Treasury of Lives Core Ontology enables the description of historical and contemporary figures in the culture and religion of Tibet, Inner Asia, and the Himalaya. It is used by the Treasury of Lives born-digitial biographical collection (https://treasuryoflives.org) to publish and share its data as Linked Open Data, facilitate the consumption of external RDF, and to to enhance the narrative text with structured data."@en ;
	skos:editoralNote
		"Add rdfs:isDefinedBy using onto-toolkit."@en ,
		"Combining all terms into one module for now; may break up into separate modules later."@en
		;
	skos:editorialNote """
		Temporarily removing imports:
			<http://id.loc.gov/ontologies/bibframe-2-1-0> ,
			<http://purl.bdrc.io/ontology/core> to make Protege easier to read."""@en ;
	skos:prefLabel "Treasury of Lives Core Ontology"@en ;
	gist:license "TBD"@en ;
	.

gist:EntityActiveStatus
	a owl:Class ;
	rdfs:subClassOf gist:Category ;
	skos:definition "Indicates whether an entity is active or inactive."@en ;
	skos:prefLabel "Entity Active Status"@en ;
	skos:scopeNote "Can be applied to many types of entities."@en ;
	.

tolo:Author
	a owl:Class ;
	rdfs:subClassOf tolo:Person ;
	skos:definition "A person who has authored or contributed to authoring at least one work."@en ;
	skos:prefLabel "Author"@en ;
	.

tolo:AuthorStatement
	a owl:Class ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			gist:Text
			[
				a owl:Restriction ;
				owl:onProperty [
					owl:inverseOf tolo:hasStatement ;
				] ;
				owl:someValuesFrom tolo:Biography ;
			]
		) ;
	] ;
	skos:definition "A disclaimer or disclosure about the author of a biography."@en ;
	skos:example "A statement indicating that the author of the biography is a relative or student of its subject."@en ;
	skos:prefLabel "Author Statement"@en ;
	.

tolo:Authorship
	a owl:Class ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			gist:TemporalRelation
			[
				a owl:Restriction ;
				owl:onProperty gist:hasParticipant ;
				owl:someValuesFrom tolo:Author ;
			]
			[
				a owl:Restriction ;
				owl:onProperty gist:hasParticipant ;
				owl:someValuesFrom tolo:Biography ;
			]
		) ;
	] ;
	skos:definition "Relates a bio to a person who has authored it."@en ;
	skos:editorialNote
		"Though there is a display order of multiple authors of a single biography, we do not currently distinguish primary from secondary author in the model, because there may be more than two authors and we still need to specify the display order."@en ,
		"Using a simple model of authorship until there is a use case for a more complex model with contributions and roles as in BIBFRAME."@en
		;
	skos:prefLabel "Authorship"@en ;
	.

tolo:Biography
	a owl:Class ;
	rdfs:subClassOf
		gist:FormattedContent ,
		[
			a owl:Restriction ;
			owl:onProperty gist:isExpressedIn ;
			owl:hasValue media-txt:html ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gist:isAbout ;
			owl:someValuesFrom tolo:Person ;
		]
		;
	skos:definition "A written account of another person's life, formatted in HTML for publication on the Treasury of Lives website."@en ;
	skos:editorialNote "Using subclass axioms rather than equivalence because the author bios should not be inferred into the class. TODO: Generalize the model a bit for external consumption: define a generic Biography class, subclass of bf:Work (or gist:Content?), then HtmlBio as formatted html content, etc."@en ;
	skos:example "The Treasury of Lives biography of Rongzom Chokyi Zangpo is published on the Treasury of Lives website at https://treasuryoflives.org/biographies/view/Rongzom-Chokyi-Zangpo/6194."@en ;
	skos:prefLabel "Biography"@en ;
	skos:scopeNote "A biography involves more than just basic facts like education, work, relationships, and death; it portrays a person's experience of these life events."@en ;
	.

tolo:Birth
	a owl:Class ;
	rdfs:subClassOf
		gist:HistoricalEvent ,
		gist:PhysicalEvent ,
		[
			a owl:Restriction ;
			owl:onProperty tolo:isEventOf ;
			owl:allValuesFrom tolo:Person ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gist:occursIn ;
			owl:cardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty tolo:isEventOf ;
			owl:cardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty tolo:inYear ;
			owl:minCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	skos:definition "TBD"@en ;
	skos:editorialNote "Can deities have birth and death? We may need to loosen the restriction."@en ;
	skos:prefLabel "Birth"@en ;
	.

tolo:BuildingType
	a owl:Class ;
	rdfs:subClassOf
		gist:Category ,
		[
			a owl:Restriction ;
			owl:onProperty [
				owl:inverseOf gist:isCategorizedBy ;
			] ;
			owl:allValuesFrom gist:Building ;
		]
		;
	skos:definition "Indicates the specific type of a building."@en ;
	skos:example "Manor, government building, palace, temple."@en ;
	skos:prefLabel "Building Type"@en ;
	.

tolo:Death
	a owl:Class ;
	rdfs:subClassOf
		gist:HistoricalEvent ,
		gist:PhysicalEvent ,
		[
			a owl:Restriction ;
			owl:onProperty tolo:isEventOf ;
			owl:allValuesFrom tolo:Person ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gist:occursIn ;
			owl:cardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty tolo:isEventOf ;
			owl:cardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty tolo:inYear ;
			owl:minCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	skos:definition "TBD"@en ;
	skos:editorialNote "Can deities have birth and death? We may need to loosen the restriction."@en ;
	skos:prefLabel "Death"@en ;
	.

tolo:HistoricalPeriod
	a owl:Class ;
	rdfs:subClassOf gist:TimeInterval ;
	skos:definition "A named time interval with historical significance."@en ;
	skos:example "The 10th century, the eary/mid/late 19th century."@en ;
	skos:prefLabel "Historical Period"@en ;
	skos:scopeNote "Whereas a time interval is any period of time, such as 2-4pm this afternoon or 1848-1947, a historical period is named and cited in relation to historical events or culture. Historical periods include full and partial centuries, which are ordinally numbered."@en ;
	.

tolo:Incarnation
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty [
				owl:inverseOf gist:hasMember ;
			] ;
			owl:allValuesFrom tolo:IncarnationLineage ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty gist:providesOrderFor ;
			owl:allValuesFrom tolo:Person ;
		]
		;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			gist:OrderedMember
			[
				a owl:Restriction ;
				owl:onProperty [
					owl:inverseOf gist:hasMember ;
				] ;
				owl:someValuesFrom tolo:IncarnationLineage ;
			]
			[
				a owl:Restriction ;
				owl:onProperty gist:providesOrderFor ;
				owl:someValuesFrom tolo:Person ;
			]
			[
				a owl:Restriction ;
				owl:onProperty gist:sequence ;
				owl:someValuesFrom xsd:integer ;
			]
		) ;
	] ;
	skos:definition "The relationship connecting a person to an incarnation lineage and expressing the sequence and ordinal of the person in the lineage."@en ;
	skos:editorialNote "Connects person to lineage."@en ;
	skos:example "The Third Dalai Lama."@en ;
	skos:prefLabel
		"Incarnation"@en ,
		"sprul sku"@bo-x-ewts
		;
	skos:scopeNote "The sequence and ordinal may differ in some cases, and in some cases there may be no ordinal."@en ;
	.

tolo:IncarnationLineage
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty gist:hasMember ;
		owl:allValuesFrom tolo:Incarnation ;
	] ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			gist:OrderedCollection
			[
				a owl:Restriction ;
				owl:onProperty gist:hasMember ;
				owl:someValuesFrom tolo:Incarnation ;
			]
		) ;
	] ;
	skos:definition "A consecutive line of people regarded as incarnations of a non-living person or mythical figure."@en ;
	skos:example "The Dalai Lamas, the Karmapas."@en ;
	skos:prefLabel
		"Incarnation Lineage"@en ,
		"skye brgyud"@bo-x-ewts
		;
	skos:scopeNote "An incarnation lineage is enumerated chronologically and can branch into multiple lines. Usually associated with a seat, such as a monastery, and a tradition."@en ;
	.

tolo:Institution
	a owl:Class ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			gist:Organization
			[
				a owl:Restriction ;
				owl:onProperty gist:isCategorizedBy ;
				owl:someValuesFrom tolo:InstitutionType ;
			]
		) ;
	] ;
	skos:definition "A human-made organization typically connected to a single or multiple physical buildings with a shared mission, identity, norms and rules that often contains subsidiary members or suborganizations."@en ;
	skos:example "Choding Monastery, Nyarongsha School, Mentsikhang Hospital."@en ;
	skos:prefLabel "Institution"@en ;
	skos:scopeNote "An institution is not a place per se - e.g., a monastery could move to a different site - but is associated with a place."@en ;
	.

tolo:InstitutionType
	a owl:Class ;
	rdfs:subClassOf
		gist:Category ,
		[
			a owl:Restriction ;
			owl:onProperty [
				owl:inverseOf gist:isCategorizedBy ;
			] ;
			owl:allValuesFrom tolo:Institution ;
		]
		;
	skos:definition "Indicates the specific type of an institution."@en ;
	skos:example "Monastery, hospital, school, etc."@en ;
	skos:prefLabel "Institution Type"@en ;
	skos:scopeNote "Using the class-type pattern, some institution types will also be represented as subclasses because they have specialized restrictions."@en ;
	.

tolo:KinshipGroup
	a owl:Class ;
	rdfs:subClassOf
		gist:Collection ,
		[
			a owl:Restriction ;
			owl:onProperty gist:hasMember ;
			owl:allValuesFrom tolo:Person ;
		]
		;
	skos:definition "A social unit consisting of people with a common ancestor."@en ;
	skos:example "The Adro clan; the Jangling family."@en ;
	skos:prefLabel "Kinship Group"@en ;
	skos:scopeNote "Includes Tibetan concepts that may individually mean something more specific, such as: rig (rigs) for clan, race, family; gyu (rgyud) for lineage, descent; dungrab (gdung rabs) for descendants, lineage, generation; dung (gdung) for progeny, descendants, lineage, descent line; ru (rus) for bone, bloodline, lineage, family, clan; khang (khang) for household and tsang (tshang) for family and household."@en ;
	.

tolo:KinshipGroupType
	a owl:Class ;
	owl:equivalentClass [
		owl:intersectionOf (
			gist:Category
			[
				a owl:Restriction ;
				owl:onProperty [
					owl:inverseOf gist:isCategorizedBy ;
				] ;
				owl:allValuesFrom tolo:KinshipGroup ;
			]
		) ;
	] ;
	skos:definition "A type of kinship group."@en ;
	skos:example "Lhasa Aristocracy."@en ;
	skos:prefLabel "Kinship Group Type"@en ;
	skos:scopeNote "Currently only one type is defined."@en ;
	.

tolo:LandmarkType
	a owl:Class ;
	rdfs:subClassOf
		gist:Category ,
		[
			a owl:Restriction ;
			owl:onProperty [
				owl:inverseOf gist:isCategorizedBy ;
			] ;
			owl:allValuesFrom gist:Landmark ;
		]
		;
	skos:definition "Indicates the specific type of a landmark."@en ;
	skos:example "Stupa, hermitage, lake, mountain, hidden land, cave"@en ;
	skos:prefLabel "Landmark Type"@en ;
	.

tolo:Marriage
	a owl:Class ;
	owl:equivalentClass [
		owl:intersectionOf (
			gist:Agreement
			gist:TemporalRelation
			[
				a owl:Restriction ;
				owl:onProperty gist:hasParty ;
				owl:allValuesFrom tolo:Person ;
			]
			[
				a owl:Restriction ;
				owl:onProperty gist:isCategorizedBy ;
				owl:onClass tolo:MarriageRank ;
				owl:cardinality "1"^^xsd:nonNegativeInteger ;
			]
			[
				a owl:Restriction ;
				owl:onProperty gist:hasParty ;
				owl:onClass tolo:Person ;
				owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
			]
			[
				a owl:Restriction ;
				owl:onProperty tolo:familyName ;
				owl:cardinality "1"^^xsd:nonNegativeInteger ;
			]
			[
				a owl:Restriction ;
				owl:onProperty tolo:familyName ;
				owl:cardinality "1"^^xsd:nonNegativeInteger ;
			]
		) ;
	] ;
	skos:definition "A culturally and often legally recognized union between two people, called spouses, that establishes rights and obligations between them."@en ;
	skos:editorialNote
		"Cardinality restriction on gist:hasParty of 2 generates Protege error, so using qualified cardinality restriction instead."@en ,
		"Predicates should be directed from the relationship to the other objects, to support named graph drops to delete the relationship."@en
		;
	skos:prefLabel "Marriage"@en ;
	.

tolo:MarriageRank
	a owl:Class ;
	rdfs:subClassOf
		gist:Category ,
		[
			a owl:Restriction ;
			owl:onProperty [
				owl:inverseOf gist:isCategorizedBy ;
			] ;
			owl:allValuesFrom tolo:Marriage ;
		]
		;
	skos:definition "A category differentiating senior (first) from subsequent (junior) marriages in polygamous relationships."@en ;
	skos:prefLabel "Marriage Rank"@en ;
	skos:scopeNote "The associated spouse is termed a junior or senior wife/husband. There is only one senior wife or husband; all others are uniformly designated junior."@en ;
	.

tolo:MarriageTerminationCause
	a owl:Class ;
	rdfs:subClassOf
		gist:Category ,
		[
			a owl:Restriction ;
			owl:onProperty [
				owl:inverseOf gist:isCategorizedBy ;
			] ;
			owl:allValuesFrom tolo:Marriage ;
		]
		;
	skos:definition "The reason for the termination of a marriage."@en ;
	skos:example "Death, divorce, dissolution."@en ;
	skos:prefLabel "Marriage Termination Cause"@en ;
	.

tolo:Monastery
	a owl:Class ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			tolo:Institution
			[
				a owl:Restriction ;
				owl:onProperty gist:isCategorizedBy ;
				owl:hasValue tolx:_InstitutionType_monastery ;
			]
		) ;
	] ;
	skos:definition "TBD"@en ;
	skos:editorialNote "Has a founder, has abotts, ..."@en ;
	skos:prefLabel "Monastery"@en ;
	.

tolo:Person
	a owl:Class ;
	rdfs:subClassOf
		bdo:Person ,
		gist:Person ,
		wiki:Person
		;
	skos:definition "An individual being who was born, lived, and died in the human realm."@en ;
	skos:prefLabel
		"Person"@en ,
		"mi"@bo-x-ewts
		;
	skos:scopeNote "May include quasi-historical figures."@en ;
	.

tolo:SourceStatement
	a owl:Class ;
	rdfs:subClassOf
		gist:Content ,
		[
			a owl:Restriction ;
			owl:onProperty gist:isAbout ;
			owl:someValuesFrom gist:Content ;
		]
		;
	skos:definition "A statement of the sources used to compile a piece of content."@en ;
	skos:editorialNote "Could alternatively model with an object property hasSourceStatement."@en ;
	skos:prefLabel "Source Statement"@en ;
	.

tolo:Summary
	a owl:Class ;
	rdfs:subClassOf
		gist:Content ,
		[
			a owl:Restriction ;
			owl:onProperty gist:isAbout ;
			owl:someValuesFrom gist:Content ;
		]
		;
	skos:definition "A brief statement or restatement of the main points of a larger work."@en ;
	skos:editorialNote "Could alternatively model with an object property hasSummary."@en ;
	skos:prefLabel "Summary"@en ;
	.

tolo:TeacherStudentRelationship
	a owl:Class ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			gist:TemporalRelation
			gist:Agreement
			[
				a owl:Restriction ;
				owl:onProperty gist:hasGiver ;
				owl:someValuesFrom tolo:Person ;
			]
			[
				a owl:Restriction ;
				owl:onProperty gist:hasRecipient ;
				owl:someValuesFrom tolo:Person ;
			]
			[
				a owl:Restriction ;
				owl:onProperty gist:conformsTo ;
				owl:someValuesFrom tolo:Tradition ;
			]
		) ;
	] ;
	skos:definition "A time-delimited relationship between a teacher and a student."@en ;
	skos:prefLabel "Teacher-Student Relationship"@en ;
	.

tolo:Tradition
	a owl:Class ;
	rdfs:subClassOf gist:Intention ;
	skos:definition "An instance of Tibetan religion that identifies as a cohesive group with distinct modes of ritual and learning, as well as associated institutions such as monasteries."@en ;
	skos:example "The major schools and subschools of Tibetan Buddhism as well as Bon."@en ;
	skos:prefLabel
		"Tradition"@en ,
		"chos lugs"@bo-x-ewts
		;
	.

tolo:actualYear
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf tolo:inYear ;
	skos:definition "The year in which something existed or took place."@en ;
	skos:example "Gyergom Tsultime Sengge was born in 1144 and died in 1204."@en ;
	skos:prefLabel "actual year"@en ;
	.

tolo:alternateYear
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf tolo:inYear ;
	skos:definition "The year in which something is proposed to have existed or taken place."@en ;
	skos:example "According to one source, Botrul Dongak Tenpai Nyima was born in 1900, while according to another source his birth year was XXX and he died in 1902. No indication of greater or less likelihood of the two possibilities is implied."@en ;
	skos:prefLabel "alternate year"@en ;
	skos:scopeNote "Differs from tolo:approximateYear, which specifies a single year but is intended to be inexact. In this case, various scholars have proposed different years. If there is an alternate year, there is no actual year; rather, both proposed dates are asserted as alternates."@en ;
	.

tolo:approximateYear
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf tolo:inYear ;
	skos:definition "The approximate year in which something existed or took place."@en ;
	skos:example "The birth event of Lhagyari Namgyel Gyatso took place in approximately 1927."@en ;
	skos:prefLabel "approximate year"@en ;
	skos:scopeNote "Differs from tolo:alternateYear, which is used when there is more than one proposed year. In this case, only one year is asserted, but it is intended to be inexact, thus encompassing some number of years before and/or after."@en ;
	.

tolo:birthFamilyName
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf tolo:familyName ;
	rdfs:domain tolo:Person ;
	rdfs:range rdf:langString ;
	skos:definition "The family name a person is assigned at birth."@en ;
	skos:prefLabel "birth family name"@en ;
	skos:scopeNote "A family name other than the birth name can be acquired through marriage."@en ;
	.

tolo:dissolutionYear
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf gist:actualEndYear ;
	skos:definition "The year an agreement such as marriage was ended. In the case of marriage, this refers to divorce rather than the end of a marriage by death of one of the spouses."@en ;
	skos:editorialNote
		"Use if we need to distinguish divorce from death as the end of a marriage."@en ,
		"We may want to do this like birth and death - have a Dissolution object with a year. If there are many things with actual/approximate/alternate values, (dissolution, founding, etc.) we should establish this as a general pattern."@en
		;
	skos:prefLabel "dissolution year"@en ;
	.

tolo:existsDuring
	a owl:ObjectProperty ;
	skos:definition "Relates an entity to an event, time period, or other temporal object during which it does or did exist."@en ;
	skos:example "Trimalo lived during the 7th century; Dorje Sherab lived during the 12th and 13th centuries."@en ;
	skos:prefLabel "exists during"@en ;
	skos:scopeNote "The subject of tolo:existsDuring is a non-temporal resource. The property contrasts with tolo:occursDuring, which relates a temporal resource to a containing temporal resource."@en ;
	gist:domainIncludes
		gist:LivingThing ,
		gist:PhysicalIdentifiableItem ,
		tolo:Person
		;
	gist:rangeIncludes
		gist:Event ,
		gist:TemporalRelation ,
		gist:TimeInterval ,
		tolo:HistoricalPeriod
		;
	.

tolo:familyName
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf gist:name ;
	rdfs:domain [
		a owl:Class ;
		owl:unionOf (
			tolo:Person
			tolo:Marriage
		) ;
	] ;
	rdfs:range rdf:langString ;
	skos:definition "The name a person bears based on his/her genealogic or marital relationships."@en ;
	skos:prefLabel "family name"@en ;
	skos:scopeNote "Also applies to marriages; in an ordinary marriage, the marriage, spouses, and children take the husband's family name; in a makpa marriage, the wife's. The marriage name can be inferred from the individuals' family names, or vice versa."@en ;
	.

tolo:firstName
	a owl:DatatypeProperty ;
	rdfs:subClassOf gist:name ;
	rdfs:domain tolo:Person ;
	skos:definition "The first name of a person."@en ;
	skos:prefLabel "first name"@en ;
	skos:scopeNote "Neutral as to whether the first name is the given or family name."@en ;
	.

tolo:foundingYear
	a owl:DatatypeProperty ;
	rdfs:subPropertyOf gist:actualStartYear ;
	skos:definition "TODO"@en ;
	skos:editorialNote "We may want to follow the pattern of Birth and Death here, instead of just using a property. Depends on what other information is available. We could have Founding with an agent and a date."@en ;
	skos:example "The Tshal-pa tradition was founded in 1123."@en ;
	skos:prefLabel "founding year"@en ;
	.

tolo:fullName
	a owl:DatatypeProperty ;
	rdfs:subClassOf gist:name ;
