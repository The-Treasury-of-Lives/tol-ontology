	skos:prefLabel "exists during"@en ;
	skos:scopeNote "The subject of tolo:existsDuring is a non-temporal resource. The property contrasts with tolo:occursDuring, which relates a temporal resource to a containing temporal resource."@en ;
	gist:domainIncludes
		gist:LivingThing ,
		gist:PhysicalIdentifiableItem ,
		tolo:Person
		;
	gist:rangeIncludes
		gist:Event ,
		gist:TemporalRelation ,
		gist:TimeInterval ,
		tolo:HistoricalPeriod
		;
	.

