<https://w3id.org/treasuryoflives/taxonomy/tolRanks>
	a owl:Ontology ;
	owl:imports <https://w3id.org/treasuryoflives/ontology/tolCoreX.x.x> ;
	owl:versionIRI <https://w3id.org/treasuryoflives/taxonomy/tolRanksX.x.x> ;
	skos:definition "Defines non-numeric rank categories such as primary and secondary."@en ;
	skos:editorialNote "Hand-curated."@en ;
	skos:prefLabel "Ranks"@en ;
	.

tolx:_Rank_primary
	a tolo:Rank ;
	skos:definition "First in importance or significance."@en ;
	skos:example "The primary image of a person or place"@en ;
	skos:prefLabel "Primary"@en ;
	skos:scopeNote "Used to control search results: the primary image should appear in search results for the person or place."@en ;
	.

tolx:_Rank_secondary
	a tolo:Rank ;
	skos:definition "Second or non-primary in importance or significance."@en ;
	skos:example "A secondary image of a person or place"@en ;
	skos:prefLabel "Secondary"@en ;
	skos:scopeNote "Used to control search results: only the primary image should appear in search results for the person or place."@en ;
	.

