tolx:_MappingPriority_4
	a tolo:MappingPriority ;
	skos:prefLabel "Lower"@en ;
	tolo:minZoomLevel "20"^^xsd:integer ;
	tolo:priority "4"^^xsd:integer ;
	.

tolx:_MappingPriority_5
	a tolo:MappingPriority ;
	skos:prefLabel "Lower still"@en ;
