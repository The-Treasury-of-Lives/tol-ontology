<https://w3id.org/treasuryoflives/data/tolReferenceCollections>
	a owl:Ontology ;
	owl:deprecated "true"^^xsd:boolean ;
	owl:imports
		<https://w3id.org/treasuryoflives/ontology/tolCoreX.x.x> ,
		<https://w3id.org/treasuryoflives/ontology/tolDisplayX.x.x>
		;
	owl:versionIRI <https://w3id.org/treasuryoflives/data/tolReferenceCollectionsX.x.x> ;
	skos:definition "Defines online reference collections used in seeAlso links from resources such as persons and images. The link to a specific resource is constructed from the collection url and the local name of the reference; display labels are used for link text."@en ;
	skos:editorialNote
		"Hand-curated."@en ,
		"The url should end in a slash to facilitate URL construction."@en
		;
	skos:prefLabel "Treasury of Lives Reference Collections"@en ;
	.

told:_ReferenceCollection_har
	a tolo:ReferenceCollection ;
	owl:deprecated "true"^^xsd:boolean ;
	skos:altLabel "HAR"@en ;
	skos:prefLabel "Himalayan Art Resources"@en ;
	tolo:displayLabel "View Image at HAR"@en ;
	tolo:url "https://www.himalayanart.org/"^^xsd:anyURI ;
	.

told:_ReferenceCollection_lotsawa
